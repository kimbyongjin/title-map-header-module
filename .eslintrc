{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "warn",
      "objects": "warn",
      "imports": "never",
      "exports": "never",
      "functions": "never"
  }],
    "react/jsx-filename-extension": 0,
    "no-unused-vars": [
      "error", {
        "vars": "local",
        "args": "none"
      }
    ]
  }
}
